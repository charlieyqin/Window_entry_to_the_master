ar r miracl.a mrbrick.o mrebrick.o mrecgf2m.o mrmuldv.o mrshs512.o
rm mr*.o
gcc -I. bmark.c miracl.a -o bmark
gcc -I. fact.c miracl.a -o fact
g++  -I. -c -O2 big.cpp
